<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
	<servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.RevMetrix.servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EstablishmentRegServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.RevMetrix.servlet.EstablishmentRegServlet</servlet-class></servlet>
  <servlet>
    <servlet-name>AccountServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.RevMetrix.servlet.AccountServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GameServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.RevMetrix.servlet.GameServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>StatsServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.RevMetrix.servlet.StatsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AccountSignUpServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.RevMetrix.servlet.AccountSignUpServlet</servlet-class>
  </servlet>
  	
  	<servlet>
  	<servlet-name>StartBowlingServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.RevMetrix.servlet.StartBowlingServlet</servlet-class>
  	</servlet>
  	
  	<servlet>
  	<servlet-name>EventServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.RevMetrix.servlet.EventServlet</servlet-class>
  	</servlet>
  	
  	<servlet>
  	<servlet-name>ShotServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.RevMetrix.servlet.ShotServlet</servlet-class>
  	</servlet>
  	
  	<servlet>
  	<servlet-name>BallArsenalServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.RevMetrix.servlet.BallArsenalServlet</servlet-class>
  	</servlet>
  	<servlet>
    <servlet-name>SessionServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.RevMetrix.servlet.SessionServlet</servlet-class>
  	</servlet>

  <!-- Servlet mappings -->
	<servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EstablishmentRegServlet</servlet-name>
  	<url-pattern>/establishmentReg</url-pattern></servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AccountServlet</servlet-name>
  	<url-pattern>/logIn</url-pattern></servlet-mapping>
  	
  	  <servlet-mapping>
  	<servlet-name>AccountSignUpServlet</servlet-name>
  	<url-pattern>/signUp</url-pattern></servlet-mapping>
  	
  	 <servlet-mapping>
  	<servlet-name>StatsServlet</servlet-name>
  	<url-pattern>/stats</url-pattern></servlet-mapping>
  	
  	<servlet-mapping>
  	<servlet-name>StartBowlingServlet</servlet-name>
  	<url-pattern>/startBowling</url-pattern></servlet-mapping>
  	  	<servlet-mapping>
  	<servlet-name>GameServlet</servlet-name>
  	<url-pattern>/game</url-pattern></servlet-mapping>
  	
  	<servlet-mapping>
  	<servlet-name>ShotServlet</servlet-name>
  	<url-pattern>/shot</url-pattern></servlet-mapping>
  	
  	<servlet-mapping>
  	<servlet-name>BallArsenalServlet</servlet-name>
  	<url-pattern>/ballArsenal</url-pattern></servlet-mapping>
  	
  	<servlet-mapping>
  	<servlet-name>EventServlet</servlet-name>
  	<url-pattern>/event</url-pattern></servlet-mapping>
  
    <servlet-mapping>
  	<servlet-name>SessionServlet</servlet-name>
  	<url-pattern>/session</url-pattern></servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
